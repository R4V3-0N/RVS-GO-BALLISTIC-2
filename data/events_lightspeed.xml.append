<?xml version="1.0" encoding="UTF-8"?>
<FTL><!--appending to Inferno-Core-->
	<mod:findName type="event" name="FMCORE_ATLAS_EQUIPMENT">
		<mod-append:choice req="GB_CAN_CALL_DISPATCH" blue="false">
			<text>[GB] Contact a dispatcher for a delivery job.</text>
			<event>
				<loadEvent>GB_ATLAS_EQUIPMENT_LIST</loadEvent>
				<variable name="gb_called_dispatch" op="set" val="1"/>
			</event>
		</mod-append:choice>
		<mod-append:choice hidden="true" req="gb_called_dispatch" blue="false">
			<text>You have already contacted a dispatcher.</text>
			<event load="OPTION_INVALID"/>
		</mod-append:choice>
	</mod:findName>
	<mod:findName type="event" name="FMCORE_SPAWNER">
		<mod-append:queueEvent>GB_DISPATCH_SPAWNER</mod-append:queueEvent>
	</mod:findName>
</FTL>


